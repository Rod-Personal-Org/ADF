{
	"name": "CopyKOFData",
	"properties": {
		"activities": [
			{
				"name": "Copy_s1l",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "files//Datos.txt"
					},
					{
						"name": "Destination",
						"value": ".KofDatos"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"maxConcurrentConnections": 2
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings",
							"skipLineCount": 0
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"tableOption": "autoCreate"
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": false,
					"logStorageSettings": {
						"linkedServiceName": {
							"referenceName": "KofFilesRepository",
							"type": "LinkedServiceReference"
						},
						"path": "logs"
					},
					"validateDataConsistency": true,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Id",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Id",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "desc",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "desc",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "price",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "price",
									"type": "String"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_s1l",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_s1l",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}